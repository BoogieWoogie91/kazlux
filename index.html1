	}
}     
</script>

<script>
	function get_cookie ( cookie_name ) {
  var results = document.cookie.match ( '(^|;) ?' + cookie_name + '=([^;]*)(;|$)' );
 
  if ( results )
    return ( unescape ( results[2] ) );
  else
    return null;  
}
	var x = get_cookie ( "city" );
  console.log(x);

  if (x=='Алматы') {
  	window.location.replace("http://kz-luxury.com/homsters/index_alm.html");	
  } else if (x=='Астана') {
  	window.location.replace("http://kz-luxury.com/homsters/index_ast.html");   	
  }
</script>

